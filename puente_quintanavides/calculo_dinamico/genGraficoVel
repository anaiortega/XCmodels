#!/usr/bin/gnuplot -persist
set encoding iso_8859_1
set terminal postscript eps color enhanced
set output "output.eps"
set xlabel "Velocidad (km/h)" 0.000000,0.000000;
set ylabel "Máximo desplazamiento vertical (m)" 0.000000,0.000000;
set title "Desplazamiento máximo en centro de vano" 0.000000,0.000000;
plot "fMaximaTalgo350.dat" with lines;
#    EOF
