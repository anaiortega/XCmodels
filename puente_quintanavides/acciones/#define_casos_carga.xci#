\def_prop["listaAcciones","list"]{}
\mdlr{\loads
  {
    \constant_ts["cts"]{ \factor{1.0} } \c{Time series: constant_ts[nombre]{factor}}
     \set_current_time_series{"cts"}

    \load_pattern["RETRACC"]{}
    \load_pattern["FLU"]{}

    \load_pattern["G0"]{} \c{Peso propio de la sección prefabricada}
    \load_pattern["G1"]{} \c{Peso propio de la losa superior y de la carga muerta}
    \load_pattern["TC1V1"]{} \c{Tren de cargas 1 en vía 1}
    \load_pattern["TC1V2"]{} \c{Tren de cargas 1 en vía 2}
    \load_pattern["TC2V1"]{} \c{Tren de cargas 2 en vía 1}
    \load_pattern["TC2V2"]{} \c{Tren de cargas 2 en vía 2}
    \load_pattern["TC3V1"]{} \c{Tren de cargas 3 en vía 1}
    \load_pattern["TC3V2"]{} \c{Tren de cargas 3 en vía 2}
    \load_pattern["FV1"]{} \c{Frenado en vía 1}
    \load_pattern["FV2"]{} \c{Frenado en vía 2}
    \load_pattern["ARRV1"]{} \c{Arranque en vía 1}
    \load_pattern["ARRV2"]{} \c{Arranque en vía 2}
    \load_pattern["LZV1"]{} \c{Efecto de lazo en vía 1}
    \load_pattern["LZV2"]{} \c{Efecto de lazo en vía 2}
    \load_pattern["VTRSV"]{} \c{Viento transversal}
    \load_pattern["VLONG"]{} \c{Viento longitudinal}
    \load_pattern["NV"]{} \c{Nieve}
    \load_pattern["AD"]{} \c{Descarrilo}
    \c{\load_pattern["PCE"]{} \c{Prueba de carga estática}
    \load_pattern["PCD"]{} \c{Prueba de carga dinámica}}

\c{
    \linear_ts["lts"]{ \factor{1.0} } \c{Time series: linear_ts[nombre]{factor}}
    \set_current_time_series{"lts"}
    \load_pattern["IncT"]{} \c{Incremento de temperatura}
    \load_pattern["DecT"]{} \c{Decremento de temperatura}}

    \set{listaAcciones= @listaNombresLoadPatterns()}
  }}
\def_prop["numAcciones","double"]{@listaAcciones.size}
\def_prop["nPaso","double"]{0}
