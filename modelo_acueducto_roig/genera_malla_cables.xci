def pesoPropioCables(A,B,C):

    cad

        # Peso propio de los cables
        linea[A]

            pesoPorNodo= pesoTirante*longElem
            for_each_interior_nod
              ( load([0,0,pesoPorNodo,0,0,0]) )

        linea[B]

            pesoPorNodo= pesoTirante*longElem
            for_each_interior_nod
              ( load([0,0,pesoPorNodo,0,0,0]) )

        linea[C]

            pesoPorNodo= pesoTirante*longElem
            for_each_interior_nod
              ( load([0,0,pesoPorNodo,0,0,0]) )




